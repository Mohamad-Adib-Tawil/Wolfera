    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter (Pixel 9a)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/main.dart",
            "args": [
              "-d",
              "emulator-5554"
            ]
          },
        {
            "name": "wolfera",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
              "--dart-define=GOOGLE_IOS_CLIENT_ID=${env:GOOGLE_IOS_CLIENT_ID}",
              "--dart-define=GOOGLE_WEB_CLIENT_ID=${env:GOOGLE_WEB_CLIENT_ID}",
              "--dart-define=FIREBASE_IOS_API_KEY=${env:FIREBASE_IOS_API_KEY}",
              "--dart-define=FIREBASE_IOS_APP_ID=${env:FIREBASE_IOS_APP_ID}",
              "--dart-define=FIREBASE_IOS_MSG_SENDER_ID=${env:FIREBASE_IOS_MSG_SENDER_ID}",
              "--dart-define=FIREBASE_IOS_PROJECT_ID=${env:FIREBASE_IOS_PROJECT_ID}",
              "--dart-define=FIREBASE_IOS_STORAGE_BUCKET=${env:FIREBASE_IOS_STORAGE_BUCKET}"
            ]
        },
        {
            "name": "wolfera (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main.dart",
            "args": [
              "--dart-define=GOOGLE_IOS_CLIENT_ID=${env:GOOGLE_IOS_CLIENT_ID}",
              "--dart-define=GOOGLE_WEB_CLIENT_ID=${env:GOOGLE_WEB_CLIENT_ID}",
              "--dart-define=FIREBASE_IOS_API_KEY=${env:FIREBASE_IOS_API_KEY}",
              "--dart-define=FIREBASE_IOS_APP_ID=${env:FIREBASE_IOS_APP_ID}",
              "--dart-define=FIREBASE_IOS_MSG_SENDER_ID=${env:FIREBASE_IOS_MSG_SENDER_ID}",
              "--dart-define=FIREBASE_IOS_PROJECT_ID=${env:FIREBASE_IOS_PROJECT_ID}",
              "--dart-define=FIREBASE_IOS_STORAGE_BUCKET=${env:FIREBASE_IOS_STORAGE_BUCKET}"
            ]
        },
        {
            "name": "wolfera (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart",
            "args": [
              "--dart-define=GOOGLE_IOS_CLIENT_ID=${env:GOOGLE_IOS_CLIENT_ID}",
              "--dart-define=GOOGLE_WEB_CLIENT_ID=${env:GOOGLE_WEB_CLIENT_ID}",
              "--dart-define=FIREBASE_IOS_API_KEY=${env:FIREBASE_IOS_API_KEY}",
              "--dart-define=FIREBASE_IOS_APP_ID=${env:FIREBASE_IOS_APP_ID}",
              "--dart-define=FIREBASE_IOS_MSG_SENDER_ID=${env:FIREBASE_IOS_MSG_SENDER_ID}",
              "--dart-define=FIREBASE_IOS_PROJECT_ID=${env:FIREBASE_IOS_PROJECT_ID}",
              "--dart-define=FIREBASE_IOS_STORAGE_BUCKET=${env:FIREBASE_IOS_STORAGE_BUCKET}"
            ]
        }
    ]
}